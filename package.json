{
  "name": "js2coffee",
  "version": "2.1.0",
  "description": "JavaScript to CoffeeScript compiler",
  "homepage": "http://js2.coffee",
  "main": "dist/js2coffee.js",
  "keywords": [
    "javascript",
    "coffeescript",
    "language",
    "compiler"
  ],
  "scripts": {
    "test": "mocha",
    "autotest": "mocha -R min -b --watch",
    "prepublish": "mocha >/dev/null && make -B dist"
  },
