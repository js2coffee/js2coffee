    "esprima": "2.6.0",
    "estraverse": "1.9.3",
    "minimist": "1.2.0",
    "read-input": "0.2.0",
    "source-map": "0.1.43"
  },
  "devDependencies": {
    "browserify": "5.13.1",
    "chai": "1.10.0",
    "coffee-script": "1.10.0",
    "coffeeify": "1.1.0",
    "coffeelint": "1.12.1",
    "glob": "4.5.3",
    "js-yaml": "3.4.2",
    "mocha": "2.3.3",
    "mocha-clean": "0.4.0",
    "uglify-js": "2.4.24",
    "underscore": "1.8.3"
  }
