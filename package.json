  "license": "MIT",
  "bugs": {
    "url": "https://github.com/js2coffee/js2coffee/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/js2coffee/js2coffee.git"
  },
  "browser": "js2coffee.coffee",
  "bin": {
    "js2coffee": "./bin/js2coffee"
  },
  "dependencies": {
